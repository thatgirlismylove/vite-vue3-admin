{
  "name": "vue3-js-element-admin",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "build:dev": "NODE_ENV=production vite build --mode dev",
    "build:test": "NODE_ENV=production vite build --mode test",
    "build:prod": "NODE_ENV=production vite build --mode prod",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "check-files": "run-s lint format",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@unocss/reset": "^0.58.3",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "element-plus": "^2.8.1",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^6.2.1",
    "pinia": "^2.2.2",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qs": "^6.13.0",
    "terser": "^5.31.6",
    "vue": "^3.5.3",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@iconify-json/ep": "^1.2.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@unocss/preset-wind": "^0.58.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.9.4",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.78.0",
    "unocss": "^0.58.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-icons": "^0.19.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.4.3"
  }
}
